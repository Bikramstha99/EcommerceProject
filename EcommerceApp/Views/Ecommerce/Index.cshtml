@model List<EcommerceApp.Models.Domain.Ecommerce>
@{

}
<h1>Ecommerce</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Category</th>
            <th>Price</th>
            
            <th> </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Category</td>
                <td>@item.Price</td>
                <td><a href="Ecommerce/View/@item.Id"><button type="submit">View</a></td>
                <td><a href="Ecommerce/Delete/@item.Id"><button type="submit" >Delete</a></td>

            </tr>
        }
    </tbody>
</table>